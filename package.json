  "dependencies": {
    "actions-on-google": "^2.13.0",
    "body-parser": "^1.19.0",
    "dialogflow": "^1.2.0",
    "dialogflow-fulfillment": "^0.6.1",
    "eslint": "^7.17.0",
    "express": "^4.17.1",
    "lint": "^0.7.0",
    "mongoose": "^5.11.10"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
